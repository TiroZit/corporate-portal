		"@nuxt/ui": "2.14.2",
		"@nuxtjs/color-mode": "3.3.2",
		"@nuxtjs/tailwindcss": "6.11.4",
		"eslint": "8.57.0",
		"husky": "9.0.11",
		"lint-staged": "15.2.2",
		"nuxt": "3.10.3",
		"nuxt-quasar-ui": "2.0.7",
		"shadcn-nuxt": "0.10.0",
		"sugarss": "4.0.1"
	}
