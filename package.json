{
	"name": "nuxt-app",
	"type": "module",
	"version": "1.0.0",
	"private": false,
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"build": "vue-tsc --noEmit && npm run lint && nuxt build",
		"dev": "nuxt dev",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"postinstall": "nuxt prepare",
		"lint": "eslint --fix . && stylelint --fix **/*.{sass,vue}",
		"pre-commit": "lint-staged --allow-empty --config lint-staged.config.js",
		"prepare": "husky install"
	},
	"browserslist": [
		">1%",
		"not op_mini all",
