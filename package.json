		"appwrite": "14.0.0",
		"class-variance-authority": "0.7.0",
		"clsx": "2.1.0",
		"radix-vue": "1.5.1",
		"tailwind-merge": "2.2.1",
		"tailwindcss-animate": "1.0.7",
		"vee-validate": "4.12.6",
		"vue": "3.4.21",
		"vue-router": "4.3.0",
		"zod": "3.22.4"
	},
	"devDependencies": {
		"@antfu/eslint-config": "2.8.0",
